<form action="/expense/filter/{{categoryId}}" method="get">
    <h1>Fitering</h1>

    <table>
        <tr>
            <th>Description</th>
            <th>Totals</th>
        </tr>

        {{#each category}}
        <tr>
            <td>
                {{expense}}
            </td>
            <td>
                {{total}}
            </td>
        </tr>
        {{/each}}
    </table>

    <a href="/" class="text-[#065A82] font-bold text-2xl">Back to home</a>
</form>